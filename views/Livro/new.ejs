<h1>Edit/Inclua um Livro</h1>

<form action= "/livro/create" method="POST" class="form-signin">
	<h2 class="form-signin-heading"> Create a book</h2>
	<div class="control-group">
	<input type="text" class="form-control" placeholder="titulo" name="titulo">
	</div>
	<div class="control-group">
	<input type="text" class="form-control" placeholder="editora" name="editora">
	</div>
	<div class="control-group">
	<input type="text" class="form-control" placeholder="edicao" name="edicao">
	</div>
	<div class="control-group">
	<input type="text" class="form-control" placeholder="partoOf" name="partOf">
	<br />
	</div>
	
	<input type="submit" class="btn btn-lg btn-primary btn-block" value="Create Book"/>
	<input type="hidden" name="_csrf" value="<%=_csrf%>">
	</form>
	
	
	
	
	